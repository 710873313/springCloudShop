<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.persistence.IntegralChangeMapper">
    <resultMap id="BaseResultMap" type="com.spring.domain.model.IntegralChange">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="changeName" property="changeName" jdbcType="VARCHAR" />
        <result column="changeDep" property="changeDep" jdbcType="VARCHAR" />
        <result column="createTime" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="updateTime" property="updateTime" jdbcType="TIMESTAMP"/>
        <result column="status" property="status" jdbcType="INTEGER" typeHandler="com.spring.domain.type.handler.IntegralChangeStatusHandler"/>
    </resultMap>

    <sql id="base_column">
        id,change_name changeName,change_dep changeDep,create_time createTime,update_time updateTime,status
    </sql>

    <insert id="addIntegralChange" useGeneratedKeys="true" keyProperty="id">
        insert into t_integral_change(change_name,change_dep,create_time,update_time,status)
        VALUE (#{changeName,jdbcType=VARCHAR},#{changeDep,jdbcType=VARCHAR},#{createTime,jdbcType=TIMESTAMP},#{updateTime,jdbcType=TIMESTAMP},#{status,jdbcType=INTEGER,typeHandler=com.spring.domain.type.handler.IntegralChangeStatusHandler})
    </insert>

</mapper>